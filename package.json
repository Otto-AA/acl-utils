    "babel-polyfill": "^6.26.0",
    "docsify-cli": "^4.3.0",
    "jest": "^24.8.0",
    "jsdoc-to-markdown": "^5.0.0",
    "regenerator-runtime": "^0.13.2",
    "standard": "^12.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "n3": "^1.1.1"
  },
  "standard": {
